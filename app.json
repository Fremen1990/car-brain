{
  "expo": {
    "name": "car-brain",
    "slug": "car-brain",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/images/carBrainIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/carBrainLogoSmall.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "This app requires access to the camera.",
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/carBrainLogoSmall.png",
        "backgroundColor": "#161622"
      },
      "package": "com.anonymous.carBrain"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/carBrainIcon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
